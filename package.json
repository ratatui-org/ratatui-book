{
  "name": "ratatui.rs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "_format": "prettier . --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte --plugin=prettier-plugin-organize-imports --plugin=prettier-plugin-tailwindcss",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --check",
    "preinstall": "npx playwright install"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.2",
    "@astrojs/check": "^0.7.0",
    "@astrojs/partytown": "^2.1.0",
    "@astrojs/starlight": "^0.25.0",
    "@astrojs/starlight-tailwind": "^2.0.3",
    "@astrojs/tailwind": "^5.1.0",
    "@expressive-code/plugin-collapsible-sections": "^0.35.3",
    "astro": "^4.11.5",
    "astro-diagram": "^0.7.0",
    "hast-util-select": "^6.0.2",
    "hastscript": "^9.0.0",
    "marked": "^13.0.2",
    "rehype-mermaid": "^2.1.0",
    "remark-svgbob": "^1.1.1",
    "sharp": "^0.33.4",
    "starlight-links-validator": "^0.9.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "verbose-regexp": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "playwright": "^1.45.1",
    "prettier": "3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "remark": "^15.0.1",
    "remark-emoji": "^5.0.0",
    "remark-youtube": "^1.3.2",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
